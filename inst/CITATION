citHeader("To cite rhosp in publications use:")


citEntry(entry="Manual",
         title = "rhosp: an actuary approach of risk evaluation during hospitalizations.",
         author = "Julie Barth\'es, Christophe Dutang",
	 year = 2018,
         note = "R package version 1.10",
         textVersion =
	 paste(	"J. Barthes and C. Dutang",
		"(2018),",
	       	"rhosp: an actuary approach of risk evaluation during hospitalizations.")
)

